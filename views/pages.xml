    <record id="contact_page" model="website.page">
        <field name="name">Contact us</field>
        <field name="website_published">True</field>
        <field name="url">/contact</field>
        <field name="type">qweb</field>
        <field name="key">theme_seccua.contact_page</field>
        <field name="arch" type="xml">
            <t t-name="theme_seccua.contact_page_template"> 
            <t t-call="website.layout">
                <section class="row container-1470 header_bg section-pt section-pb">
                    <div class="col-md-6">
                        <form action="/website_form/" method="post" data-model_name="crm.lead" data-success_page="/page/website_crm.contactus_thanks" class="s_website_form form-horizontal container-fluid mt32" enctype="multipart/form-data">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                            <div class="row">
                                <div class="col-md-6">
                                    <i class="fas fa-envelope-open-text"></i>
                                    <div class="group">
                                        <label class="vedor-label">Name</label>
                                        <input class="input-form" type="text" required="True" id="username" name="contact_name"/>
                                        <span class="highlight"></span>
                                        <span class="bar"></span>
                                    </div>
                                    <div class="group">
                                        <label class="vedor-label">Phone</label>
                                        <input class="input-form" type="text" name="phone" required="True"/>
                                        <span class="highlight"></span>
                                        <span class="bar"></span>
                                    </div>
                                    <div class="group">
                                        <label class="vedor-label">Email</label>
                                        <input class="input-form" type="text" required="True" id="email" name="email_from"/>
                                        <span class="highlight"></span>
                                        <span class="bar"></span>
                                    </div>
                                    <div class="group">
                                        <label class="vedor-label">Company Name</label>
                                        <input class="input-form" type="text" name="partner_name" required="True"/>
                                        <span class="highlight"></span>
                                        <span class="bar"></span>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                        <div class="group">
                                            <label class="vedor-label">Subject</label>
                                            <input class="input-form" type="text" name="name" required="True"/>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="group">
                                            <label class="vedor-label">Your inquiry</label>
                                            <textarea class="input-form" name="description" required="True"/>
                                            <span class="highlight"></span>
                                            <span class="bar"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                    <button class="btn btn-primary btn-lg o_website_form_send">Send us your inquiry</button>
                                    <span id="o_website_form_result"></span>
                                    <span class="highlight">If you ‘re a job seeker,  please apply from <a href="#">here</a></span>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-md-6">
                        <div class="website_infos">
                            <h3><span t-field="res_company.name"/></h3>
                            <div class="address">
                                <span t-field="res_company.street"/> <span t-field="res_company.street2"/> <span t-field="res_company.city"/>
                            </div>
                            <div class="phone">
                                <i class="fas fa-phone"></i> <span t-field="res_company.phone"/>
                            </div>
                            <div class="email">
                                <i class="fas fa-envelope-open"></i> <span t-field="res_company.email"/>
                            </div>
                            <div class="social-link">
                                <a t-if="website.social_facebook" t-att-href="website.social_facebook" class="btn btn-sm btn-link"><i class="fab fa-facebook"/></a>
                                <a t-if="website.social_twitter" t-att-href="website.social_twitter" class="btn btn-sm btn-link"><i class="fab fa-twitter"/></a>
                                <a t-if="website.social_linkedin" t-att-href="website.social_linkedin" class="btn btn-sm btn-link"><i class="fab fa-linkedin"/></a>
                                <a t-if="website.social_youtube" t-att-href="website.social_youtube" class="btn btn-sm btn-link"><i class="fa fa-youtube-play"/></a>
                                <a t-if="website.social_github" t-att-href="website.social_github" class="btn btn-sm btn-link"><i class="fab fa-github"/></a>
                                <a t-if="website.social_instagram" t-att-href="website.social_instagram" class="btn btn-sm btn-link"><i class="fab fa-instagram"/></a>
                            </div>
                        </div>
                        <img class="steps mt64 mb64" src="/theme_seccua/static/src/images/steps.svg"/>
                    </div>
                </section>
                <div class="oe_structure" />
                </t>
            </t>
        </field>
    </record>

    
